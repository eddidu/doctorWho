{
  "name": "doctorWho",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "star": "node ./bin/www",
    "start:dev": "npm run watch && nodemon --ext js,jade ./bin/www",
    "build": "browserify ./client/main.js -o ./public/javascripts/bundle.js",
    "watch": "npm run watch:client & npm run watch-server",
    "watch-server": "nodemon --ext js,jade ./bin/www",
    "watch:client": "watchify ./client/main.js -o ./public/javascripts/bundle.js -v",
    "test": "mocha --recursive"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "jquery": "^2.1.4",
    "mongoose": "^4.3.3",
    "morgan": "~1.6.1",
    "request": "^2.67.0",
    "serve-favicon": "~2.3.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "browserify": "^12.0.1",
    "chai": "^3.4.1",
    "mocha": "^2.3.4",
    "mockery": "^1.4.0",
    "nodemon": "^1.8.1",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "watchify": "^3.6.1"
  }
}
